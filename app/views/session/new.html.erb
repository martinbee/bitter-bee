<div class="col-md-6 col-md-offset-3">
  <h1 class="page-header jumbotron ">Sign In</h1>
  <%= simple_form_for(:session, url: :auth) do |f| %>
      <%= f.input :email %>
      <%= f.input :password %>
      <%= f.submit "Let Me In", class: "btn btn-primary" %>
  <% end %>
  New User?<%= link_to "Sign up!", :sign_up %>
</div>
